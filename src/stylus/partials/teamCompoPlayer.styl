.teamCompoPlayer
  position relative
  display inline-block
  padding 1rem
  width 100px
  font-size .9rem
  svg
    display block
    margin 0 auto
    margin-bottom 5px
    height 45px
  path
    fill #AAA
  .number
    position absolute
    top 30px
    left 0
    width 100%
    text-align center
    display block
    color white
    font-weight 600
    font-size .9rem
    text-shadow 0 1px 1px rgba(black, .5)
