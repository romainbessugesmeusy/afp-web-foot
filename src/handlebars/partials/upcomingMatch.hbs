<div class="upcomingMatch" data-id="{{id}}">
    {{#if home.id}}
        <a href="/teams/{{home.id}}" class="home">{{home.name}}</a>
    {{else}}
        <span class="home unknown"></span>
    {{/if}}
    <span class="time fontFigures">{{time}}</span>
    {{#if away.id}}
        <a href="/teams/{{away.id}}" class="away">{{away.name}}</a>
    {{else}}
        <span class="away unknown"></span>
    {{/if}}
</div>