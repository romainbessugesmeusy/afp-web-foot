<div class="upcomingMatch">
    <span class="home {{#unless home.name}}unknown{{/unless}}">
        <span class="name">{{home.name}}</span>
    </span>
    <span class="time fontFigures">{{time}}</span>
    <span class="away {{#unless away.name}}unknown{{/unless}}">
        <span class="name">{{away.name}}</span>
    </span>
</div>